{
  "name": "line-tvbot",
  "version": "1.0.0",
  "private": true,
  "description": "TV programs notification bot for LINE",
  "license": "MIT",
  "author": {
    "name": "nokazn",
    "url": "https://github.com/nokazn"
  },
  "main": "handler.js",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier ./{,**/}*.{t,j}s --write",
    "format:check": "yarn format --check",
    "spell-check": "cspell ./{,**/}*.{t,j}s",
    "lint": "eslint ./*.{t,j}s ./src/{,**/}*.ts --ignore-path .eslintignore --report-unused-disable-directives",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" serverless offline",
    "deploy": "cross-env NODE_ENV=production serverless deploy"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@swc/core": "^1.2.124",
    "@swc/jest": "^0.2.15",
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@types/serverless": "^1.78.40",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "cross-env": "^7.0.3",
    "cspell": "^5.15.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.4",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "serverless": "^2.70.0",
    "serverless-create-global-dynamodb-table": "^3.1.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.3.1",
    "serverless-prune-plugin": "^2.0.0",
    "serverless-webpack": "^5.6.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
